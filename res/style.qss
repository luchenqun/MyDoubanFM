}

QComboBox{
        border:1px solid #C9CACA;
        color:#727171;
        font-size:16px
}
QComboBox QAbstractItemView::item{
        height:30px;
}
QComboBox::drop-down{
        width:20px;
        border:none;
}
QComboBox::down-arrow{
    image:url(:/img/UI_SZ_XXSZ_CS_XL);
}

QRadioButton
{
    font-size:16px;
    color:#727171;
}

QRadioButton::indicator {
    width: 25px;
    height: 25px;
}

QRadioButton::indicator::unchecked {
    image: url(:/img/UI_SZ_XXSZ_XZK_WXZZT);
}

QRadioButton::indicator::checked {
    image: url(:/img/UI_SZ_XXSZ_XZK_XZZT);
}

QDateEdit{
    border:1px solid #C9CACA;
    font-size:16px;
    color:#727171;
}

QSplitter::handle{
	background:lightgray;
}

QMenu{
    background:white;
	border:1px solid lightgray;
}
QMenu::item{
	padding:0px 100px 0px 20px;
	height:25px;
}
QMenu::item:selected:enabled{
    background:lightgray;
        color:white;
}
QMenu::item:selected:!enabled{
    background:transparent;
}
QMenu::separator{
    height:1px;
    background:lightgray;
	margin:2px 0px 2px 0px;
}

QMenu#menu{
    background:white;
	border:1px solid lightgray;
}
QMenu#menu::item{
        padding:0px 20px 0px 20px;
        height:28px;
        font-size:12px;
        color:#773300;
}
QMenu#menu::item:selected:enabled{
    background:#FFBB61;
    margin:1px 1px 1px 1px;
}
QMenu#menu::item:selected:!enabled{
    background:transparent;
}
QMenu#menu::separator{
    height:1px;
    background:lightgray;
	margin:2px 0px 2px 0px;
}
QMenu#menu::indicator {
    padding:10px;
}

QToolTip{
	border:0px solid red;
	background:#FFE9D4;
	color:773300;
	font-size:12px;
	margin:1px;
        border-radius: 3px;
}

QLineEdit{
	border:1px solid rgb(180, 180, 180);
}

QGroupBox::title{
	color:green;
}

QTabWidget::pane{
	border:none;
}
QTabWidget::tab-bar{
	alignment:center;
}
QTabBar::tab{
	background:transparent;
	color:white;
	min-width:30ex;
	min-height:10ex;
}
QTabBar::tab:hover{
	background:rgb(255, 255, 255, 100);
}
QTabBar::tab:selected{
	border-color: white;
        background: white;
	color:green;
}

QListWidget#noBorderListWidget{
